{
    "keyboard_name": "Skeletyl Elite-C",
    "usb": {
        "device_version": "2.0.0"
    },
    "features": {
        "bootmagic": true,
        "mousekey": true,
        "extrakey": true,
        "rgb_matrix": false
    },
    "ws2812": {
        "pin": "D3"
    },
    "rgb_matrix": {
        "driver": "ws2812"
    },
    "matrix_pins": {
        "cols": ["B6", "B2", "B3", "B1", "F7", "F6"],
        "rows": ["B5", "B4", "E6", "D7"]
    },
    "diode_direction": "ROW2COL",
    "split": {
        "enabled": true,
        "soft_serial_pin": "D2"
    },
    "processor": "atmega32u4",
    "bootloader": "atmel-dfu"
}
